{
	"compilerOptions": {
		"target": "es6",
		"module": "commonjs",
		"moduleResolution": "node",
		"baseUrl": ".",
		"rootDirs": ["src", "out"],
		"outDir": "out",
		"paths": {
			"vscode": ["node_modules/vscode"]
		},
		"forceConsistentCasingInFileNames": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"pretty": true,
		"declaration": true,
		"skipLibCheck": true
	}
}
